{
  "name": "pikkr-cli",
  "description": "Get a random element from an array, on the command line.",
  "version": "0.0.0",
  "author": "Melanie Seltzer @melanieseltzer",
  "bin": {
    "pikkr-cli": "./bin/run"
  },
  "bugs": "https://github.com/melanieseltzer/pikkr-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.5.8",
    "@oclif/config": "^1.12.4",
    "@oclif/plugin-help": "^2.1.6",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@oclif/test": "^1.2.2",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.21",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "prettier": "^1.16.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.3.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/melanieseltzer/pikkr-cli",
  "keywords": [
    "oclif"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT",
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "oclif": {
    "bin": "pikkr-cli"
  },
  "repository": "melanieseltzer/pikkr-cli",
  "scripts": {
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "types": "lib/index.d.ts"
}
